.maternity {
  margin-bottom: 60px;

  &__content {
    display: flex;
    justify-content: space-between;
    align-items: center;
  }
  &__text {
    width: 43%;

    & > p {
      margin-top: 20px;
      font-weight: 400;
      line-height: 1.3;
    }
  }
  &__img {
    position: relative;
    right: -5%;
    width: 59%;

    & > img {
      width: 100%;
      object-fit: contain;
    }
  }
}